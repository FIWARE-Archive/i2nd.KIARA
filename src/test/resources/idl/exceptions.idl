exception DividedByZeroException {
	i32 code;
	string message;
};

exception ExceptionOne {
	i32 code;
	string message;
};

exception ExceptionTwo {
	i32 code;
	string message;
};

service Exceptions {
  float32 divide (float32 n1, float32 n2) raises (DividedByZeroException); 
  i32 function() raises (ExceptionOne, ExceptionTwo);
};
